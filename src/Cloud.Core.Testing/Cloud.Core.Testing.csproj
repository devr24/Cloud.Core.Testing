<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFramework>net5</TargetFramework>
    <IsPackable>true</IsPackable>
    <PackageId>Cloud.Core.Testing</PackageId>
    <GeneratePackageOnBuild>false</GeneratePackageOnBuild>
    <PackageTags>Cloud.Core.Testing Xunit Testing FluentAssertions Cloud.Core</PackageTags>
    <PackageReleaseNotes>Full API Documentation can be found here:
https://cloud1core.blob.core.windows.net/docs/Cloud.Core.Testing/index.html</PackageReleaseNotes>
    <Description>Utility test package used in all Cloud.Core projects. Contains extensions to Xunit and helper methods.</Description>
    <Authors>Robert McCabe</Authors>
    <Company>Aylesbury Solutions Ltd</Company>
    <Copyright>Aylesbury Solutions Ltd</Copyright>
    <Version>5.0</Version>
    <PackageIconUrl>https://cloud1core.blob.core.windows.net/icons/CC.png</PackageIconUrl>
    <ProjectGuid>{234E5079-4489-43F7-BBE5-4DCF47256636}</ProjectGuid>
	  <RepositoryUrl>https://github.com/rmccabe24/Cloud.Core.Testing</RepositoryUrl>
    <RepositoryType>Private</RepositoryType>
    <DocumentationFile>Cloud.Core.Testing.xml</DocumentationFile>
    <PackageProjectUrl>https://dev.azure.com/cloudcoreproject/CloudCore/</PackageProjectUrl>
    <PackageRequireLicenseAcceptance>true</PackageRequireLicenseAcceptance>
	<PackageLicenseExpression>MIT</PackageLicenseExpression>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="FluentAssertions" Version="6.0.0" />
    <PackageReference Include="xunit" Version="2.4.1" />
  </ItemGroup>
</Project>
